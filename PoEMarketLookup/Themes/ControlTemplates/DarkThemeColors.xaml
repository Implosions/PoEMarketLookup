<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PoEMarketLookup.Themes.ControlTemplates">

    <SolidColorBrush x:Key="Control_Background" Color="#2d3237"/>
    <SolidColorBrush x:Key="Control_Foreground" Color="White"/>
    <SolidColorBrush x:Key="Control_Background_Hover" Color="#918e86"/>
    <SolidColorBrush x:Key="Control_Foreground_Disabled" Color="#FF838383"/>
    <SolidColorBrush x:Key="Control_Background_Disabled" Color="#252426"/>
    <SolidColorBrush x:Key="Control_Border" Color="#ff707070"/>
    <SolidColorBrush x:Key="Control_Background_Pressed" Color="#918e86"/>

</ResourceDictionary>